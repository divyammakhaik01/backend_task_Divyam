  <body>
    <div>
      <div >
        <form class="row g-3 note-form">
          <div class="col-auto">
            <input
              type="text"
              class="form-control input"
              id="inputPassword2"
              placeholder="Enter Something.."
            />
          </div>

          <div class="col-auto">
            <button type="submit" class="btn btn-primary mb-3 submit">
              SUBMIT
            </button>
          </div>
        </form>
      </div>
      <div class="notes-list">
          <a href="http://localhost:8080/fetchAllTasks">Goto Notes</a>
