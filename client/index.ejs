      crossorigin="anonymous"
    ></script>

    <!-- Socketio Connection -->
    <script>
      let socket = io.connect("http://localhost:8080");
      socket.on("connect_error", (err) => {});

      // on connect
      socket.on("connect", () => {
        console.log("connected with socket ", socket.id);
      });

